<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tk.app.mapper.IAddress">

    <select id="find" resultType="int">
        select count(*) as cnt from tab_order_payments
    </select>

    <select id="add" resultType="int" parameterType="com.tk.app.domain.Address">

        insert into ${tableName} (orderNo,address) values(#{orderNo}, #{address})

    </select>

</mapper>
